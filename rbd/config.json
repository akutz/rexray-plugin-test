{
      "Args": {
        "Description": "",
        "Name": "",
        "Settable": null,
        "Value": null
      },
      "Description": "REX-Ray standalone for Ceph RBD",
      "Documentation": "https://github.com/codedellemc/rexray-plugin",
      "Entrypoint": [
        "/usr/bin/rexray", "service", "start", "-f", "-l", "debug"
      ],
      "Env": [
        {
          "Description": "",
          "Name": "LIBSTORAGE_SERVICE",
          "Settable": [
            "value"
          ],
          "Value": "rbd"
        },
        {
          "Description": "", 
          "Name": "RBD_DEFAULTPOOL",
          "Settable": [
            "value"
          ],
          "Value": "rbd"
        }
      ],
      "Interface": {
        "Socket": "rexray.sock",
        "Types": [
          "docker.volumedriver/1.0"
        ]
      },
      "Linux": {
        "AllowAllDevices": true,
        "Capabilities": ["CAP_SYS_ADMIN"],
        "Devices": null
      },
      "Mounts": [
        {
          "Source": "/dev",
          "Destination": "/dev",
          "Type": "bind",
          "Options": ["rbind"]
        },
        {
          "Source": "/sys",
          "Destination": "/sys",
          "Type": "bind",
          "Options": ["rbind"]
        }, 
		{
          "Source": "/etc/ceph",
          "Destination": "/etc/ceph",
          "Type": "bind",
          "Options": ["rbind"]
        }
      ],
      "Network": {
        "Type": "host"
      },
      "PropagatedMount": "/var/lib/libstorage/volumes",
      "User": {},
      "WorkDir": ""
}
